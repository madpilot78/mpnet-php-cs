{
    "name": "madpilot78/mpnet-php-cs",
    "description": "Custom coding standard sniffs",
    "type": "phpcodesniffer-standard",
    "keywords": [
        "phpcs",
        "phpcs-standard",
        "trailing-commas"
    ],
    "license": "UNLICENSE",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "madpilot78\\MPNetPHPCS\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Guido Falsi",
            "email": "mad@madpilot.net"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^7.0",
        "symfony/var-dumper": "^5.3"
    },
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
